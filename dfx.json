{
    "canisters": {
        "Account": {
            "type": "custom",
            "build": "python3 -m kybra account src/account/account.py src/account/account.did",
            "candid": "src/account/account.did",
            "wasm": ".kybra/account/account.wasm.gz",
            "declarations": {
                "output": "api/Canisters/account",
                "node_compatibility": true
            }
        },
        "Balance": {
            "type": "custom",
            "build": "python3 -m kybra Balance src/Balance/grantInitialBalance.py src/Balance/grantInitialBalance.did",
            "candid": "src/Balance/grantInitialBalance.did",
            "wasm": ".kybra/Balance/Balance.wasm.gz",
            "declarations": {
                "output": "api/Canisters/Balance",
                "node_compatibility": true
            }
        },
        "Claim": {
            "type": "custom",
            "build": "python3 -m kybra Balance src/claim/claim.py src/claim/claim.did",
            "candid": "src/claim/claim.did",
            "wasm": ".kybra/claim/claim.wasm.gz",
            "declarations": {
                "output": "api/Canisters/claim",
                "node_compatibility": true
            }
        }
    }
}