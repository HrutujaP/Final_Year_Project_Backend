type Storage = record {
  Space : int;
  Path : text;
  Rent : int;
  RenterPrincipal : principal;
  OwnerName : text;
  TimePeriod : text;
};
service : () -> {
  addRentee : (principal, principal) -> (opt principal);
  deleteAdvertisement : (principal) -> (opt text);
  getAdvertisement : (principal) -> (opt Storage) query;
  postAdvertisement : (int, text, text, text, int) -> (opt principal);
  removeRentee : (principal) -> (opt principal);
}